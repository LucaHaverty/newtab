{
    "name": "Simple New Tab",
    "description": "better new tab page",
    "version": "1.1",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "128": "images/icon128.png",
        "256": "images/icon256.png",
        "512": "images/icon512.png"
    },
    "incognito": "split",
    "permissions": [
        "storage"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["/thirdParty/jquery.min.js"]
    }],

    "manifest_version": 3
}